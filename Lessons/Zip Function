x = [1,2,3,4]
y = [1,1,3,7,9]
c = ["one", "two", "four","five"
    ]
     
for i in range(min(len(x),len(y))):
    if x[i] == y[i]:
        print("Same")
    else:
        print("Not the Same")

#zip function is better way to do this
        
#takes any method of itterable objects and puts them together in tuples
print(list(zip(x,y)))
     
print(list(zip(x,y,c)))

for i,j in zip(x,y):
    if i == j:
        print("same")
    else:
        print("Not same")

